		local tors = CloneChar:WaitForChild("Torso")
		local huma = CloneChar:WaitForChild("Humanoid")
		huma.DisplayName = " "
		local neck = Instance.new("Weld",tors)
		neck.Part0 = tors
		neck.Part1 = head
		neck.C0 = CF(0,1.5,0)
		local nek = neck.C1
		neck.Enabled = true
		local root = CloneChar.HumanoidRootPart.RootJoint.C0
